            $(\'#viewprojectModal\').modal(\'hide\');
            $(\'#editprojectModal\').modal(\'show\');
        });

        $(document).on(\'click\', \'.view-project\', function(e) {
            e.preventDefault();
            const projectID = $(this).data(\'projectid\');
            const projectname = $(this).data(\'projectname\');
            const createdon = $(this).data(\'createdon\');
            $(\'.projectID\').val(projectID);
            $(\'.projectname\').val(projectname);
            $(\'.createdon\').val(createdon);
            $(\'#viewprojectModal\').modal(\'show\');
        });

        $(document).on(\'click\', \'.delete-project\', function(e) {
            e.preventDefault();
            const projectID = $(this).data(\'projectid\');
            $(\'.projectID\').val(projectID);
            // console.log(projectID);
            $(\'#deleteprojectModal\').modal(\'show\');
        });

        $(document).on(\'click\', \'.enable-project\', function(e) {
            e.preventDefault();
            const projectID = $(this).data(\'projectid\');
            $(\'.projectID\').val(projectID);
            // console.log(projectID);
            $(\'#enableprojectModal\').modal(\'show\');
        });

        $(document).on(\'click\', \'.disable-project\', function(e) {
            e.preventDefault();
            const projectID = $(this).data(\'projectid\');
            $(\'.projectID\').val(projectID);
            // console.log(projectID);
            $(\'#disableprojectModal\').modal(\'show\');
        });
    });
</script>
